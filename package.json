{
  "name": "nexus7995-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:api": "nest build api",
    "build:web": "nest build web",
    "build": "nest build web && nest build api",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:api:prod": "cross-env NODE_ENV=production node dist/apps/api/main",
    "start:web:prod": "cross-env NODE_ENV=development node dist/apps/web/main",
    "start": "cross-env NODE_ENV=development nest start api",
    "start:api": "cross-env NODE_ENV=development nest start api",
    "start:api:dev": "cross-env NODE_ENV=development nest start api --watch",
    "start:api:debug": "cross-env NODE_ENV=development nest start api --debug --watch",
    "start:test": "cross-env NODE_ENV=development nest start test",
    "start:test:dev": "cross-env NODE_ENV=development nest start test --watch",
    "start:test:debug": "cross-env NODE_ENV=development nest start test --debug --watch",
    "start:web": "cross-env NODE_ENV=development nest start web",
    "start:web:dev": "cross-env NODE_ENV=development nest start web --watch",
    "start:web:debug": "cross-env NODE_ENV=development nest start web --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --detectOpenHandles --config ./apps/nexus7995-backend/test/jest-e2e.json",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js  -d libs/shared/src/core/database/config/index.ts",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate -p -o",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "typeorm:migration:revert": "npm run typeorm -- migration:revert"
  },
  "dependencies": {
    "@fastify/secure-session": "^6.0.0",
    "@json2csv/plainjs": "^6.1.2",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.1",
    "@nestjs/schedule": "^2.2.3",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/terminus": "^9.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.3.9",
    "@sendgrid/mail": "^7.7.0",
    "app": "^0.1.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cookie-parser": "^1.4.6",
    "cron": "^2.3.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "express-session": "^1.17.3",
    "fastify-formbody": "^5.3.0",
    "mariadb": "^3.0.2",
    "moment": "^2.29.4",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pdfmake": "^0.2.7",
    "randombytes": "^2.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.2",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.11",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.6",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.18",
    "@types/passport": "^1.0.12",
    "@types/passport-http": "^0.3.9",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/shared(|/.*)$": "<rootDir>/libs/shared/src/$1"
    }
  }
}
